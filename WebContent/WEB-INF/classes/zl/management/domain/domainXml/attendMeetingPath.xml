<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="zl.management.domain.filePath.AttendMeetingPath">
	<insert id="add" parameterType="AttendMeetingPath">
		insert into attendmeeting_path(did, path)
		values(#{did}, #{path})
	</insert>

	<update id="update" parameterType="AttendMeetingPath">
		update user set attendmeeting_path(path)
		values(#{path})
		where did=#{did}
	</update>

	<delete id="delete" parameterType="int">
		delete from attendmeeting_path where did = #{did}
	</delete>
	
	<delete id="deleteByPath" parameterType="string">
		delete from attendmeeting_path where path = #{path}
	</delete>
	
    <select id="list" parameterType="Map" resultType="AttendMeetingPath">
        select * from attendmeeting_path where did = #{did}
    </select>
</mapper>
